<tbody>
  <tr>
    <td><%= bill.id %></td>
    <td><%= number_to_currency(change_money(bill.total_price)) %></td>
    <td><%= bill.user_name %></td>
    <td><%= bill.user_phone %></td>
    <td><p class=<%= status_color(bill.status) %>><%= bill.status %></p></td>
    <td><%= bill.created_at.strftime('%FT%T') %></td>
    <td><%= link_to admin_bill_bookings_path(bill_id: bill.id), class: "btn btn-outline-primary waves-effect" do %>
          <i class="fa fa-eye" aria-hidden="true"></i>
        <% end %>
    </td>
    <td><%= link_to edit_admin_bill_path(bill), class: "btn btn-outline-primary waves-effect" do %>
          <i class="fa fa-edit" aria-hidden="true"></i>
        <% end %>
    </td>
  <tr>
</tbody>
