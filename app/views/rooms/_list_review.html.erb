<% @list_review.each do |review| %>
  <div class="wrap-comment">
    <span class="user"><%= review.user.name %></span>
    <span class="rating">
      <% review.rating.to_i.times do %>
        <i class="fa fa-star active"></i>
      <% end %>
      <% (Settings.stars_number_rate - review.rating.to_i).times do %>
        <i class="fa fa-star"></i>
      <% end %>
    </span>
    <p><%= review.content.html_safe %></p>
    <% if !review.photos.nil? %>
      <% review.photos.each_with_index do |image,index| %>
        <%= image_tag image.image_url, class: "img-fluid width_settup_review" %>
      <% end %>
    <% end %>
  </div>
<% end %>
