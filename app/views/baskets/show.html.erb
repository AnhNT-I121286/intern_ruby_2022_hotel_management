<%= stylesheet_link_tag "application", media: "all", "data-turbolinks-track": "reload" %>
<%= render "layouts/header" %>
<section class="signup-section" id="edit">
  <div class="container px-4 px-lg-5">
    <div class="row gx-4 gx-lg-5">
      <div class="col-md-10 col-lg-8 mx-auto text-center">
        <i class="far fa fa-shopping-basket fa-2x mb-2 text-white"></i>
        <h2 class="text-white mb-5"><%= t ".basket" %></h2>
          <div class="row input-group-newsletter">
            <ul class="bookings" id="render_bookings">
              <%= render partial: "basket", collection: @bookings %>
            </ul>
            <div id="pagy_containerr">
              <%== pagy_bootstrap_nav @pagy %>
            </div>
          </div>
      </div>
    </div>
  </div>
</section>
<%= render "layouts/footer" %>
